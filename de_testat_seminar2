#include <stdio.h>

int main()
{

	int** vector = (int**)malloc(4 * sizeof(int*));
	for (int i = 0; i < 4; i++)
	{
		*vector = (int*)malloc(3 * sizeof(int));
	}

	for (int i = 0; i < 4; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			scanf("%d", &vector[i][j]);
		}
	}

	for (int i = 0; i < 4; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			printf("%d\n", vector[i][j]);
		}
	}

	for (int i = 0; i < 4; i++)
	{
		free(vector[i]);
	}
	free(vector);

	return 0;
}
